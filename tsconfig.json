{
  "compilerOptions": {
    "incremental": true,
    "target": "es2021",
    "module": "commonjs",
    "lib": ["dom", "es2021"],
    "jsx": "react-jsx",
    "sourceMap": true,
    "strict": true,
    "rootDir": "src",
    "outDir": "lib",
    "noImplicitAny": false,
    "baseUrl": "src",
    "types": [ "node" ],
    "typeRoots": ["./node_modules/@types" ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false
  },
  "include": ["src/**/*"],
}
